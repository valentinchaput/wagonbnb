<div class="container">
  <h2 class="text-center">Here are details for your booking request:</h2>
    <div class="row">
      <div class="col-xs-12 col-sm-6 col-md-4">
        <div class="item text-center">
          <p><%= @booking.flat.title %></p>
          <p><%= @booking.user.name %></p>
          <p><%= @booking.checkin %></p>
          <p><%= @booking.checkout %></p>
          <%= link_to "new search", root_path, class: "btn btn-primary" %>
          <%= link_to "return to profile", profile_show_path, class: "btn btn-warning" %>
        </div>
      </div>
    </div>
  </div>
</div>
